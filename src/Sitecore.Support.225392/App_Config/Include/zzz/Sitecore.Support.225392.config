<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:set="http://www.sitecore.net/xmlconfig/set/">
  <sitecore>
    <settings>
      <setting name="LocationsDictionaryCacheSize" value="10000000" /> <!-- Set the Cache size value-->
    </settings>
    <pipelines>
      <startAnalytics>
        <processor type="Sitecore.Support.Analytics.Pipelines.StartAnalytics.ChangeCacheSize, Sitecore.Support.225392" patch:after="processor[@type='Sitecore.Analytics.Pipelines.StartAnalytics.CreateTracker, Sitecore.Analytics']"/>
      </startAnalytics>
    </pipelines>
  </sitecore>
</configuration>